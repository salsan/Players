{
    "name": "salsan/players",
    "description": "Library to get data about chess players from website Federazione Scacchistica Italiana",
    "type": "library",
    "keywords": ["scacchi", "fsi", "players"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Salsan\\Players\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Salvatore Santagati",
            "email": "github@salsan.dev"
        }
    ],
    "require": {
        "php": ">=8.1.10",
        "salsan/utils": "@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "9.*",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:salsan/PHPUtils.git"
        }
    ]

}
